<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sgbr.netdemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <!--    android:roundIcon="@mipmap/ic_launcher_round"-->
    <!--    android:icon="@mipmap/ic_launcher"-->
    <application
        android:allowBackup="true"
        android:icon="@drawable/space"
        android:label="@string/app_name"
        android:roundIcon="@drawable/space"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NetActivity" />
        <activity android:name=".PeopleActivity" />
        <activity android:name=".FindServerActivity" />
        <activity android:name=".FindClientActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>